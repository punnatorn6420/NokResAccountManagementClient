{
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "example": 1
    },
    "accountId": {
      "type": "integer",
      "example": 12
    },
    "message": {
      "type": "string",
      "example": "Log chang password at 2024-05-20T10:00:00Z"
    },
    "status": {
      "type": "string",
